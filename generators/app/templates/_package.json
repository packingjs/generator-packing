{
  "private": true,
  "name": "<%= props.name%>",
  "description": "",
  "version": "0.0.0",
  "repository": "zhongzhi107/packing",
  "keywords": [
    "webpack"
  ],
  "author": "zhongzhi <zhong.zhi@163.com>",
  "license": "MIT",
  "scripts": {
    "build": "better-npm-run build",
    "serve": "better-npm-run serve",
    "serve:dist": "better-npm-run build:dev && better-npm-run serve:dist",
    "eslint": "better-npm-run eslint"
  },
  "betterScripts": {
    "serve": {
      "command": "babel-node webpack/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "serve:dist": {
      "command": "babel-node webpack/webpack-prd-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/webpack.config.build.babel.js",
      "env": {
        "NODE_ENV": "production",
        "CDN_ROOT": "http://www.test1.com/prd/assets"
      }
    },
    "build:dev": {
      "command": "webpack --config webpack/webpack.config.build.babel.js",
      "env": {
        "NODE_ENV": "development",
        "CDN_ROOT": "assets"
      }
    },
    "eslint": {
      "command": "./node_modules/.bin/eslint src"
    }
  },
  "devDependencies": {<% if (props.ejs) { %>
    "ejs-loader": "^0.3.0",<% } %>
    "express": "^4.14.0",<% if (props.handlebars) { %>
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.3.0",<% } %><% if (props.html) { %>
    "html-loader": "^0.4.3",<% } %><% if (props.jade) { %>
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",<% } %><% if (props.markdown) { %>
    "markdown-loader": "^0.1.7",<% } %><% if (props.mustache) { %>
    "hogan.js": "^3.0.2",
    "mustache-loader": "^0.3.1",<% } %>
    "packing-html-webpack-plugin": "^1.0.0",
    "packing-urlrewrite": "^0.1.2",<% if (props.smarty) { %>
    "smarty-loader": "0.0.1",<% } %>
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "dependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "^0.0.9",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.0.5",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "loader-utils": "^0.2.15",
    "packing-ajax": "^0.0.1",
    "packing-rev-webpack-plugin": "^1.0.5",
    "postcss-loader": "^0.9.1",<% if (props.react) { %>
    "react": "^15.2.1",
    "react-a11y": "^0.3.3",
    "react-dom": "^15.2.1",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.2.10",<% } %>
    "replace-hash-webpack-plugin": "^1.4.5",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "engines": {
    "node": ">=0.12.7"
  }
}
